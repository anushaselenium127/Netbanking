<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="ListenerSuite"> 

  <listeners>
       <listener class-name="Llisteners.TestListener"/>
   </listeners>
  
  <test verbose="2" name="LoginTest">  
    <classes> 
      <class name="Llisteners.LoginTest"> 
      <methods>
          <include name="loginMail" />
           <include name="loginByFacebook" />
           <include name="loginByTwitter"/>
      </methods>
      </class>
    </classes> 
  </test> <!-- Test --> 
</suite> <!-- Suite --> 
